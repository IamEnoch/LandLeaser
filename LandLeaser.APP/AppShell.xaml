<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="LandLeaser.APP.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:LandLeaser.APP.Views"
    xmlns:views="using:LandLeaserApp.Views"
    xmlns:views1="clr-namespace:LandLeaserApp.Views"
    Shell.FlyoutBehavior="Flyout"
    Shell.NavBarIsVisible="True">
    
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:LoadingPage}"
        Route="LoadingPage" />
    
    <ShellContent
        Title="LoginPage"
        ContentTemplate="{DataTemplate local:LoginPage}"
        Route="LoginPage" />

    <ShellContent
        Title="SignUpPage"
        ContentTemplate="{DataTemplate local:SignUpPage}"
        Route="SignUpPage" />

</Shell>
